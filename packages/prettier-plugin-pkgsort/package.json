{
  "name": "@re-taro/prettier-plugin-pkgsort",
  "description": "Prettier plugin for sorting package.json",
  "version": "1.1.0",
  "packageManager": "pnpm@8.11.0",
  "license": "MIT",
  "author": {
    "name": "Rintaro Itokawa",
    "email": "me@re-taro.dev",
    "url": "https://re-taro.dev"
  },
  "homepage": "https://github.com/re-taro/fmt#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/re-taro/fmt.git",
    "directory": "packages/prettier-plugin-pkgsort"
  },
  "bugs": {
    "url": "https://github.com/re-taro/fmt/issues"
  },
  "keywords": [
    "format",
    "formatter",
    "json",
    "package",
    "package-json",
    "package-json",
    "prettier"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . -c",
    "lint:ts": "tsc -p . --noEmit",
    "fmt": "run-s -c fmt:*",
    "fmt:eslint": "pnpm lint:eslint --fix",
    "fmt:prettier": "prettier . -w",
    "test": "vitest"
  },
  "dependencies": {
    "prettier": "3.1.0",
    "prettier-package-json": "2.8.0"
  },
  "peerDependencies": {
    "prettier": "^3.0.0"
  }
}
