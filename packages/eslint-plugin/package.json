{
  "name": "@re-taro/eslint-plugin",
  "type": "module",
  "version": "5.1.0",
  "packageManager": "pnpm@9.7.0",
  "description": "ESLint plugin for re-taro",
  "author": "Rintaro Itokawa <me@re-taro.dev> (https://re-taro.dev)",
  "license": "MIT",
  "homepage": "https://github.com/re-taro/fmt#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/re-taro/fmt.git",
    "directory": "packages/eslint-plugin"
  },
  "bugs": {
    "url": "https://github.com/re-taro/fmt/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-plugin"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "fmt": "pnpm lint:eslint --fix",
    "lint": "pnpm run /^lint:.*/",
    "lint:eslint": "eslint .",
    "lint:ts": "tsc",
    "test": "vitest"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "devDependencies": {
    "@types/eslint": "9.6.0",
    "@types/semver": "7.5.8",
    "@typescript-eslint/parser": "8.0.1",
    "@typescript-eslint/types": "8.0.1",
    "@typescript-eslint/typescript-estree": "8.0.1",
    "@typescript-eslint/utils": "8.0.1",
    "eslint": "9.8.0",
    "eslint-vitest-rule-tester": "0.3.3",
    "semver": "7.6.3"
  }
}
