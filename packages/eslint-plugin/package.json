{
	"name": "@re-taro/eslint-plugin",
	"type": "module",
	"version": "4.0.1",
	"packageManager": "pnpm@9.5.0",
	"description": "ESLint plugin for re-taro",
	"author": "Rintaro Itokawa <me@re-taro.dev> (https://re-taro.dev)",
	"license": "MIT",
	"homepage": "https://github.com/re-taro/fmt#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/re-taro/fmt.git",
		"directory": "packages/eslint-plugin"
	},
	"bugs": {
		"url": "https://github.com/re-taro/fmt/issues"
	},
	"keywords": [
		"eslint",
		"eslint-config",
		"eslint-plugin"
	],
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "rollup -c",
		"fmt": "run-s -c fmt:*",
		"fmt:eslint": "pnpm lint:eslint --fix",
		"lint": "run-s -c lint:*",
		"lint:eslint": "eslint .",
		"lint:ts": "tsc -p . --noEmit",
		"test": "vitest"
	},
	"peerDependencies": {
		"eslint": "*"
	},
	"devDependencies": {
		"@types/eslint": "8.56.10",
		"@types/lodash.merge": "4.6.9",
		"@types/semver": "7.5.8",
		"@typescript-eslint/parser": "7.16.0",
		"@typescript-eslint/types": "7.10.0",
		"@typescript-eslint/typescript-estree": "7.12.0",
		"@typescript-eslint/utils": "7.10.0",
		"ajv": "8.16.0",
		"lodash.merge": "4.6.2",
		"semver": "7.6.2"
	}
}
