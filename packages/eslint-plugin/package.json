{
  "name": "@re-taro/eslint-plugin",
  "description": "ESLint plugin for re-taro",
  "version": "2.2.0",
  "packageManager": "pnpm@8.10.3",
  "license": "MIT",
  "author": {
    "name": "Rintaro Itokawa",
    "email": "me@re-taro.dev",
    "url": "https://re-taro.dev"
  },
  "homepage": "https://github.com/re-taro/fmt#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/re-taro/fmt.git",
    "directory": "packages/eslint-plugin"
  },
  "bugs": {
    "url": "https://github.com/re-taro/fmt/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-plugin"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "vitest"
  },
  "dependencies": {
    "@typescript-eslint/types": "6.2.1",
    "@typescript-eslint/utils": "6.2.1"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "6.10.0"
  }
}
