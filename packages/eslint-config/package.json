{
	"name": "@re-taro/eslint-config",
	"type": "module",
	"version": "4.0.0",
	"packageManager": "pnpm@8.15.5",
	"description": "ESLint config for re-taro projects",
	"author": "Rintaro Itokawa <me@re-taro.dev> (https://re-taro.dev)",
	"license": "MIT",
	"homepage": "https://github.com/re-taro/fmt#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/re-taro/fmt.git",
		"directory": "packages/eslint-config"
	},
	"bugs": {
		"url": "https://github.com/re-taro/fmt/issues"
	},
	"keywords": [
		"eslint",
		"eslint-config"
	],
	"sideEffects": false,
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "rollup -c",
		"dev": "eslint-flat-config-viewer",
		"fmt": "run-s -c fmt:*",
		"fmt:eslint": "pnpm lint:eslint --fix",
		"lint": "run-s -c lint:*",
		"lint:eslint": "eslint .",
		"lint:ts": "tsc -p . --noEmit",
		"test": "vitest"
	},
	"peerDependencies": {
		"astro-eslint-parser": "^0.16.3",
		"eslint": ">=8.40.0",
		"eslint-plugin-astro": "^0.31.4",
		"eslint-plugin-format": ">=0.1.0",
		"eslint-plugin-svelte": "^2.34.1",
		"eslint-plugin-vue": "^9.2.3",
		"eslint-processor-vue-blocks": "^0.1.1",
		"prettier-plugin-astro": "^0.13.0",
		"prettier-plugin-slidev": "^1.0.5",
		"svelte-eslint-parser": "^0.33.1",
		"vue-eslint-parser": "^9.4.2"
	},
	"peerDependenciesMeta": {
		"astro-eslint-parser": {
			"optional": true
		},
		"eslint-plugin-astro": {
			"optional": true
		},
		"eslint-plugin-format": {
			"optional": true
		},
		"eslint-plugin-svelte": {
			"optional": true
		},
		"eslint-plugin-vue": {
			"optional": true
		},
		"eslint-processor-vue-blocks": {
			"optional": true
		},
		"prettier-plugin-astro": {
			"optional": true
		},
		"prettier-plugin-slidev": {
			"optional": true
		},
		"svelte-eslint-parser": {
			"optional": true
		},
		"vue-eslint-parser": {
			"optional": true
		}
	},
	"dependencies": {
		"@antfu/eslint-define-config": "1.23.0-2",
		"@antfu/install-pkg": "0.3.1",
		"@eslint-types/jsdoc": "48.2.1",
		"@eslint-types/typescript-eslint": "7.2.0",
		"@eslint-types/unicorn": "51.0.1",
		"@re-taro/eslint-plugin": "workspace:*",
		"@stylistic/eslint-plugin": "1.7.0",
		"@typescript-eslint/eslint-plugin": "7.3.1",
		"@typescript-eslint/parser": "7.3.1",
		"eslint-config-flat-gitignore": "0.1.3",
		"eslint-define-config": "2.1.0",
		"eslint-merge-processors": "0.1.0",
		"eslint-plugin-eslint-comments": "3.2.0",
		"eslint-plugin-i": "2.29.1",
		"eslint-plugin-jsdoc": "48.2.1",
		"eslint-plugin-jsonc": "2.14.1",
		"eslint-plugin-markdown": "4.0.1",
		"eslint-plugin-n": "16.6.2",
		"eslint-plugin-no-only-tests": "3.1.0",
		"eslint-plugin-perfectionist": "2.7.0",
		"eslint-plugin-react": "7.34.1",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-react-refresh": "0.4.6",
		"eslint-plugin-toml": "0.10.0",
		"eslint-plugin-unicorn": "51.0.1",
		"eslint-plugin-unused-imports": "3.1.0",
		"eslint-plugin-vitest": "0.3.26",
		"eslint-plugin-yml": "1.13.1",
		"globals": "14.0.0",
		"jsonc-eslint-parser": "2.4.0",
		"local-pkg": "0.5.0",
		"parse-gitignore": "2.0.0",
		"prompts": "2.4.2",
		"toml-eslint-parser": "0.9.3",
		"yaml-eslint-parser": "1.2.2"
	},
	"devDependencies": {
		"@stylistic/eslint-plugin-migrate": "1.7.0",
		"@types/eslint": "8.56.6",
		"@types/fs-extra": "11.0.4",
		"@types/prompts": "2.4.9",
		"astro-eslint-parser": "0.16.3",
		"eslint-flat-config-viewer": "0.1.11",
		"eslint-plugin-astro": "0.31.4",
		"eslint-plugin-format": "0.1.0",
		"eslint-plugin-svelte": "2.35.1",
		"eslint-plugin-vue": "9.23.0",
		"eslint-processor-vue-blocks": "0.1.1",
		"eslint-ts-patch": "8.57.0-0",
		"execa": "8.0.1",
		"fast-glob": "3.3.2",
