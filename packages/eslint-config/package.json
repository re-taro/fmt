		"prettier-plugin-astro": "0.13.0",
		"prettier-plugin-slidev": "1.0.5",
		"svelte": "4.2.12",
		"svelte-eslint-parser": "0.33.1",
		"vue": "3.4.21",
		"vue-eslint-parser": "9.4.2"
	}
