{
	"name": "@re-taro/eslint-config",
	"version": "3.1.0",
	"packageManager": "pnpm@8.15.4",
	"author": "Rintaro Itokawa <me@re-taro.dev> (https://re-taro.dev)",
	"type": "module",
	"description": "ESLint config for re-taro projects",
	"keywords": [
		"eslint",
		"eslint-config"
	],
	"homepage": "https://github.com/re-taro/fmt#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/re-taro/fmt.git",
		"directory": "packages/eslint-config"
	},
	"bugs": {
		"url": "https://github.com/re-taro/fmt/issues"
	},
	"license": "MIT",
	"sideEffects": false,
	"exports": "./dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "rollup -c",
		"fmt": "run-s -c fmt:*",
		"fmt:eslint": "pnpm lint:eslint --fix",
		"fmt:prettier": "prettier . -w",
		"lint": "run-s -c lint:*",
		"lint:eslint": "eslint .",
		"lint:prettier": "prettier . -c",
		"lint:ts": "tsc -p . --noEmit"
	},
	"dependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "4.1.0",
		"@html-eslint/eslint-plugin": "0.23.1",
		"@html-eslint/parser": "0.23.0",
		"@next/eslint-plugin-next": "14.1.1",
		"@re-taro/eslint-plugin": "workspace:*",
		"@stylistic/eslint-plugin": "1.6.3",
		"@unocss/eslint-config": "0.58.5",
		"eslint-config-flat-gitignore": "0.1.3",
		"eslint-define-config": "1.24.1",
		"eslint-import-resolver-typescript": "3.6.1",
		"eslint-mdx": "3.1.5",
		"eslint-plugin-array-func": "5.0.1",
		"eslint-plugin-case-police": "0.6.1",
		"eslint-plugin-etc": "2.0.3",
		"eslint-plugin-html": "8.0.0",
		"eslint-plugin-i": "2.29.1",
		"eslint-plugin-jest-formatting": "3.1.0",
		"eslint-plugin-json-schema-validator": "4.8.3",
		"eslint-plugin-jsonc": "2.13.0",
		"eslint-plugin-jsx-a11y": "6.8.0",
		"eslint-plugin-mdx": "3.1.5",
		"eslint-plugin-n": "16.6.2",
		"eslint-plugin-no-await-in-promise": "1.1.6",
		"eslint-plugin-no-explicit-type-exports": "0.12.1",
		"eslint-plugin-no-only-tests": "3.1.0",
		"eslint-plugin-only-error": "1.0.2",
		"eslint-plugin-promise": "6.1.1",
		"eslint-plugin-react": "7.34.0",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-regexp": "2.2.0",
		"eslint-plugin-simple-import-sort": "12.0.0",
		"eslint-plugin-solid": "0.13.1",
		"eslint-plugin-storybook": "0.8.0",
		"eslint-plugin-svelte": "2.35.1",
		"eslint-plugin-toml": "0.9.2",
		"eslint-plugin-unicorn": "51.0.1",
		"eslint-plugin-unused-imports": "3.1.0",
		"eslint-plugin-vitest": "0.3.22",
		"eslint-plugin-vue": "9.22.0",
		"eslint-plugin-yml": "1.12.2",
		"globals": "14.0.0",
		"jsonc-eslint-parser": "2.4.0",
		"local-pkg": "0.5.0",
		"svelte-eslint-parser": "0.33.1",
		"toml-eslint-parser": "0.9.3",
		"typescript-eslint": "7.1.0",
		"vue-eslint-parser": "9.4.2",
		"yaml-eslint-parser": "1.2.2"
	},
	"devDependencies": {
		"@typescript-eslint/utils": "7.1.0",
		"eslint": "8.57.0"
	},
	"peerDependencies": {
		"eslint": ">=8.40.0",
		"prettier": "^3.0.0",
		"svelte": "^3.37.0 || ^4.0.0"
	}
}
