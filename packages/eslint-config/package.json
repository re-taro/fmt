{
  "name": "@re-taro/eslint-config",
  "type": "module",
  "version": "5.2.0",
  "packageManager": "pnpm@9.11.0",
  "description": "ESLint config for re-taro projects",
  "author": "Rintaro Itokawa <me@re-taro.dev> (https://re-taro.dev)",
  "license": "MIT",
  "homepage": "https://github.com/re-taro/fmt#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/re-taro/fmt.git",
    "directory": "packages/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/re-taro/fmt/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm typegen && unbuild",
    "fmt": "pnpm lint:eslint --fix",
    "lint": "pnpm run /^lint:.*/",
    "lint:eslint": "eslint .",
    "lint:ts": "tsc",
    "test": "vitest",
    "typegen": "tsx scripts/typegen.ts"
  },
  "peerDependencies": {
    "@prettier/plugin-xml": "^3.4.1",
    "@unocss/eslint-plugin": ">=0.50.0",
    "astro-eslint-parser": "^1.0.2",
    "eslint": ">=8.40.0",
    "eslint-plugin-astro": "^1.2.0",
    "eslint-plugin-format": ">=0.1.0",
    "eslint-plugin-solid": "^0.13.2 || ^0.14.0",
    "eslint-plugin-svelte": ">=2.35.1",
    "eslint-plugin-vue": "^9.27.0",
    "eslint-processor-vue-blocks": "^0.1.2",
    "prettier-plugin-astro": "^0.13.0 || ^0.14.0",
    "prettier-plugin-slidev": "^1.0.5",
    "svelte-eslint-parser": ">=0.37.0",
    "vue-eslint-parser": "^9.4.3"
  },
  "peerDependenciesMeta": {
    "@prettier/plugin-xml": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    },
    "eslint-plugin-solid": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "eslint-processor-vue-blocks": {
      "optional": true
    },
    "prettier-plugin-astro": {
      "optional": true
    },
    "prettier-plugin-slidev": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "0.4.1",
    "@clack/prompts": "0.7.0",
    "@eslint-react/eslint-plugin": "1.14.3",
    "@re-taro/eslint-plugin": "workspace:*",
    "@stylistic/eslint-plugin": "2.6.2",
    "@typescript-eslint/eslint-plugin": "8.8.0",
    "@typescript-eslint/parser": "8.8.0",
    "eslint-config-flat-gitignore": "0.3.0",
    "eslint-flat-config-utils": "0.4.0",
    "eslint-merge-processors": "0.1.0",
    "eslint-plugin-command": "0.2.6",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import-x": "4.3.1",
    "eslint-plugin-jsdoc": "50.3.0",
    "eslint-plugin-jsonc": "2.16.0",
    "eslint-plugin-markdown": "5.1.0",
    "eslint-plugin-n": "17.10.3",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-perfectionist": "3.7.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.12",
    "eslint-plugin-regexp": "2.6.0",
    "eslint-plugin-toml": "0.11.1",
    "eslint-plugin-unicorn": "55.0.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "eslint-plugin-vitest": "0.5.4",
    "eslint-plugin-yml": "1.14.0",
    "globals": "15.9.0",
    "jsonc-eslint-parser": "2.4.0",
    "local-pkg": "0.5.0",
    "parse-gitignore": "2.0.0",
    "toml-eslint-parser": "0.10.0",
    "yaml-eslint-parser": "1.2.3"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-migrate": "2.8.0",
    "@types/eslint": "9.6.1",
    "@types/fs-extra": "11.0.4",
    "@types/prompts": "2.4.9",
    "eslint-typegen": "0.3.2",
    "execa": "9.4.0",
    "fast-glob": "3.3.2",
    "fs-extra": "11.2.0",
    "jiti": "2.0.0"
  }
}
