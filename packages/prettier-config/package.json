{
	"name": "@re-taro/prettier-config",
	"version": "1.2.0",
	"packageManager": "pnpm@8.11.0",
	"author": "Rintaro Itokawa <me@re-taro.dev> (https://re-taro.dev)",
	"type": "module",
	"description": "Prettier config for re-taro",
	"homepage": "https://github.com/re-taro/fmt#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/re-taro/fmt.git",
		"directory": "packages/prettier-config"
	},
	"bugs": {
		"url": "https://github.com/re-taro/fmt/issues"
	},
	"license": "MIT",
	"sideEffects": false,
	"exports": "./dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "rollup -c",
		"fmt": "run-s -c fmt:*",
		"fmt:eslint": "pnpm lint:eslint --fix",
		"fmt:prettier": "prettier . -w",
		"lint": "run-s -c lint:*",
		"lint:eslint": "eslint .",
		"lint:prettier": "prettier . -c",
		"lint:ts": "tsc -p . --noEmit",
		"test": "prettier . --config=dist/index.js --check"
	},
	"dependencies": {
		"@re-taro/prettier-plugin-curly-and-jsdoc": "1.2.0",
		"prettier-plugin-astro": "0.12.2",
		"prettier-plugin-pkgsort": "0.2.1",
		"prettier-plugin-toml": "2.0.1"
	},
	"peerDependencies": {
		"prettier": "^3.0.0"
	}
}
