{
  "packages": {
    "packages/eslint-config": {
      "component": "eslint-config"
    },
    "packages/eslint-plugin": {
      "component": "eslint-plugin"
    },
    "packages/prettier-plugin-curly-and-jsdoc": {
      "component": "prettier-plugin-curly-and-jsdoc"
    },
    "packages/prettier-plugin-toml": {
      "component": "prettier-plugin-toml"
    }
  },
  "plugins": [
    {
      "type": "node-workspace",
      "merge": false
    },
    {
      "type": "linked-versions",
      "group-name": "eslint",
      "groupName": "eslint",
      "components": ["eslint-config", "eslint-plugin"]
    },
    {
      "type": "linked-versions",
      "group-name": "prettier",
      "groupName": "prettier",
      "components": ["prettier-plugin-curly-and-jsdoc", "prettier-plugin-toml"]
    }
  ]
}
