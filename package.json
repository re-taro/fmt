{
  "name": "@re-taro/eslint",
  "description": "My personal eslint config monorepo.",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "author": {
    "name": "Rintaro Itokawa",
    "email": "me@re-taro.dev",
    "url": "https://re-taro.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/re-taro/eslint.git"
  },
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel run stub",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@re-taro/eslint-config": "1.3.1",
    "@re-taro/eslint-plugin": "1.3.1",
    "@types/react": "18.0.26",
    "eslint": "8.31.0",
    "react": "18.2.0",
    "typescript": "4.9.4",
    "unbuild": "1.0.2"
  },
  "pnpm": {
    "overrides": {
      "@re-taro/eslint-config": "workspace:",
      "@re-taro/eslint-plugin": "workspace:"
    }
  }
}
