{
	"name": "@re-taro/fmt",
	"type": "module",
	"version": "0.0.0",
	"private": true,
	"packageManager": "pnpm@9.15.0",
	"description": "My personal formatter config monorepo.",
	"author": "Rintaro Itokawa <me@re-taro.dev> (https://re-taro.dev)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/re-taro/fmt.git"
	},
	"scripts": {
		"dev": "eslint-config-inspector --config eslint.config.js",
		"build": "pnpm -r --filter=./packages/* build",
		"fmt": "pnpm -r --parallel --filter=./packages/* fmt",
		"lint": "pnpm -r --parallel --filter=./packages/* lint",
		"test": "pnpm -r --parallel --filter=./packages/* test"
	},
	"devDependencies": {
		"@changesets/changelog-github": "0.5.0",
		"@changesets/cli": "2.27.11",
		"@eslint/config-inspector": "0.5.6",
		"@stylistic/eslint-plugin-migrate": "2.11.0",
		"@types/node": "22.10.2",
		"eslint": "9.17.0",
		"tsx": "4.19.2",
		"typescript": "5.7.2",
		"unbuild": "2.0.0",
		"vitest": "2.1.8"
	}
}
